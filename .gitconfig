[user]
    email = eisuke.kinoshita@mixi.co.jp
    name = Eisuke Kinoshita
[color]
    ui = auto
[core]
    excludesfile = /home/bkapps/.gitignore_global
    editor = vim
[alias]
    br = branch
    st = status
    co = checkout
    df = diff
    graph = log --graph
    list = config --list
    edit = config -e
    hard = reset --hard HEAD
    gedit = config --global -e
    cp = cherry-pick
[push]
    default = current
